@{
    Layout = "";
}
@model ScientificReportData.Models.Report

<style>
    ol {
        counter-reset: item
    }

    li {
        display: block

    }

        li:before {
            content: counters(item, ".") " ";
            counter-increment: item
        }
    table {
        width:95%;
        border-collapse: collapse;
    }
    li,p{padding-bottom:5px;}
    p {
        padding:0;
        padding-bottom: 5px;
    }
    tr{
        width:30%;
    }
    td{
        padding-left:5px;
    }
    table, th, td {

        border: 1px solid black;
    }
    .report-block {
        width: 90%;
      
    }
    .center {
        display: block;
        margin-left: auto;
        margin-right: auto;
    }
</style>
