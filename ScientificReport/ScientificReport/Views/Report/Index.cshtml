@{
	ViewData["Title"] = "ABTO Звіт";
}
@model User

<div class="text-center">
	<h3 class="display-5">Тут буде виводитися коротка інформація репорту</h3>
	@if (@Model != null) {
		<div>Email: @Model.Email</div>
		<a class="btn btn-success" asp-controller="Report" asp-action="CreateReport">
			<strong>Generate Report</strong>
		</a>
	}
	else {
		<div>Залогіньтеся, щоб побачити інформацію</div>
	}
</div>