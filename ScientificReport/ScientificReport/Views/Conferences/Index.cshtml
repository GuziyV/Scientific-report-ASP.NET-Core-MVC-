@{
}
@model IEnumerable<Conference>
<div class="row">
@foreach (var i in Model)
{
    
    <div class="item col-md-4  align-items-center justify-content-center text-center " style="padding:20px 0; ">
        <a asp-route-id="@i.Id" asp-controller="Conferences" asp-action="Details"><img src="@i.ImgPath" alt="" style="width:200px; height:200px; border-radius:10px; "></a>
        <h5>@i.Date</h5>
        
    </div>
}
</div>